+            ret = xsltCheckWritePath(sec, ctxt, uri->path+1);
+        else
 #endif
+        {
+            /*
+             * Check if we are allowed to write this file
+             */
+	    ret = xsltCheckWritePath(sec, ctxt, uri->path);
+        }
 
-	/*
-	 * Check if we are allowed to write this file
-	 */
-	ret = xsltCheckWritePath(sec, ctxt, uri->path);
 	if (ret <= 0) {
 	    xmlFreeURI(uri);
 	    return(ret);
